{
  "name": "today-i-learned",
  "version": "1.0.0",
  "description": "today-i-learned website",
  "browserslist": "last 10 versions",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass assets/scss/main.scss:assets/css/style.css -w",
    "compile:sass": "sass assets/scss/main.scss:assets/css/style.comp.css",
    "prefix:css": "postcss assets/css/style.comp.css --use autoprefixer -o assets/css/style.prefix.css",
    "compress:css": "node-sass assets/css/style.prefix.css assets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nadirozsoy/today-i-learned.git"
  },
  "author": "Nadir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nadirozsoy/today-i-learned/issues"
  },
  "homepage": "https://github.com/nadirozsoy/today-i-learned#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "sass": "^1.58.0"
  },
  "dependencies": {}
}
